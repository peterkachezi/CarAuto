

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link href="~/homepage/all.min.css" rel="stylesheet" />

    <link href="~/homepage/swiper-bundle.min.css" rel="stylesheet" />

    <!-- font awesome cdn link  -->
    <link href="~/homepage/all.min.css" rel="stylesheet" />

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css">
    <!-- custom css file link  -->
    <link href="~/css/style.css" rel="stylesheet" />

    <link href="~/dashboard/bootstrap.min.css" rel="stylesheet" />

    <link rel="icon" href="images/favicon.ico" type="image/ico" />

    <title>Express | Store </title>
</head>
<body>

    @await Html.PartialAsync("_Header");


    <div class="login-form-container">

        <span id="close-login-form" class="fas fa-times"></span>

        <form action="">
            <h3>user login</h3>
            <input type="email" placeholder="email" class="box">
            <input type="password" placeholder="password" class="box">
            <p> forget your password <a href="#">click here</a> </p>
            <input type="submit" value="login" class="btn">
            <p>or login with</p>
            <div class="buttons">
                <a href="#" class="btn"> google </a>
                <a href="#" class="btn"> facebook </a>
            </div>
            <p> don't have an account <a href="#">create one</a> </p>
        </form>

    </div>





        <div id="carouselExampleSlidesOnly div" class="carousel slide" data-ride="carousel">

            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img class="d-block w-100 img-fluid" src="~/slideimages/Image1.jpeg" alt="First slide" style="height: 85em; ">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100 img-fluid" src="~/slideimages/Image3.jpeg" alt="Second slide" style="height: 85em; ">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100 img-fluid" src="~/slideimages/Image4.jpeg" alt="Second slide" style="height: 85em; ">

                </div>
                <div class="carousel-item">
                    <img class="d-block w-100 img-fluid" src="~/slideimages/Image5.jpeg" alt="Second slide" style="height: 85em; ">

                </div>
            </div>
        </div>




 
    <section class="icons-container">

        <div class="icons">
            <i class="fas fa-home"></i>
            <div class="content">
                <h3>150+</h3>
                <p>branches</p>

            </div>
        </div>

        <div class="icons">
            <i class="fas fa-car"></i>
            <div class="content">
                <h3>4770+</h3>
                <p>cars sold</p>
            </div>
        </div>

        <div class="icons">
            <i class="fas fa-users"></i>
            <div class="content">
                <h3>320+</h3>
                <p>happy clients</p>
            </div>
        </div>

        <div class="icons">
            <i class="fas fa-car"></i>
            <div class="content">
                <h3>1500+</h3>
                <p>news cars</p>
            </div>
        </div>

    </section>

















    @await Html.PartialAsync("_Featured");





    @*@await Html.PartialAsync("_Featured");*@



    @await Html.PartialAsync("_Newsletter");

    @*@await Html.PartialAsync("_Reviews");*@

    @await Html.PartialAsync("_Contact");

    @await Html.PartialAsync("_Footer");



    <script src="~/homepage/swiper-bundle.min.js"></script>
    <script src="~/Scripts/jquery-3.5.1.js"></script>
    <script src="~/js/script.js"></script>

    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
</body>
</html>

